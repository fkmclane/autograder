{
	"project": {
		"file": "students.json",
		"method": "json",
		"name": "fizzbuzz",
		"testdir": "project"
	},
	"clean": {
		"method": "noop"
	},
	"build": {
		"method": "docker",
		"dockerfile": ".autograder/Dockerfile",
		"timeout": 10
	},
	"tests": [
		{
			"run": { 
				"method": "docker",
				"command": "/autograder/tests/runner.sh /autograder/tests/tests.bats",
				"constraints": "--net=none",
				"timeout": 10
			},
			"score": {
				"method": "points",
				"points_possible": 40,
				"points_each": 5
			}
		},
		{
			"run": { 
				"method": "docker",
				"command": "/autograder/tests/runner.sh /autograder/tests/files.bats",
				"constraints": "--net=none",
				"timeout": 10
			},
			"score": {
				"points_possible": 5,
				"points_each": 2,
				"free_points": 1
			}
		}
	],
	"reports": [
		{
			"destination": "%u-output.txt",
			"detail": [
				"output",
				"points"
			],
			"send_method": "file",
			"formatter_method": "text",
			"seperate": true,
			"summarize": false

		},
		{
			"destination": "prof-output.json",
			"detail": [
				"results",
				"points"
			],
			"send_method": "file",
			"formatter_method": "json",
			"seperate": false,
			"summarize": true

		}
	]
}
